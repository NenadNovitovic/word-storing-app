<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainPane" prefHeight="423.0" prefWidth="328.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.NewWordController">
   <children>
      <Button alignment="CENTER" layoutX="29.0" layoutY="386.0" mnemonicParsing="false" onAction="#addWordFunction" prefHeight="17.0" prefWidth="118.0" text="Add word" textAlignment="CENTER" textOverrun="CLIP" />
      <TextField fx:id="mainLanguageTF" layoutX="14.0" layoutY="73.0" onAction="#changeFocus" promptText="Swedish" />
      <TextField fx:id="secondLanguageTF" layoutX="14.0" layoutY="112.0" onAction="#changeFocus" promptText="English" />
      <TextField fx:id="example1TF" layoutX="13.0" layoutY="154.0" onAction="#changeFocus" prefHeight="17.0" prefWidth="299.0" promptText="Example 1" />
      <TextField fx:id="example2TF" layoutX="14.0" layoutY="192.0" onAction="#changeFocus" prefHeight="17.0" prefWidth="299.0" promptText="Example 2" />
      <TextField fx:id="example3TF" layoutX="14.0" layoutY="219.0" onAction="#changeFocus" prefHeight="17.0" prefWidth="299.0" promptText="Example 3" />
      <TextField fx:id="pronounciationTF" layoutX="14.0" layoutY="248.0" onAction="#changeFocus" prefHeight="17.0" prefWidth="179.0" promptText="pronounciation" />
      <TextArea fx:id="moreInfoTA" layoutX="14.0" layoutY="285.0" prefHeight="87.0" prefWidth="200.0" promptText="More info:" wrapText="true" />
      <Button fx:id="btn1" alignment="CENTER" layoutX="150.0" layoutY="15.0" mnemonicParsing="false" onAction="#addLetter" prefHeight="20.0" prefWidth="30.0" text="Å" textAlignment="CENTER" />
      <Button fx:id="btn2" alignment="CENTER" layoutX="185.0" layoutY="15.0" mnemonicParsing="false" onAction="#addLetter" prefHeight="20.0" prefWidth="30.0" text="Ä" textAlignment="CENTER" />
      <Button fx:id="btn3" alignment="CENTER" layoutX="220.0" layoutY="15.0" mnemonicParsing="false" onAction="#addLetter" prefHeight="20.0" prefWidth="30.0" text="Ö" textAlignment="CENTER" />
      <Button alignment="CENTER" layoutX="54.0" layoutY="15.0" mnemonicParsing="false" onAction="#changeCaps" prefHeight="20.0" text="Change caps" textAlignment="CENTER" />
   </children>
</AnchorPane>
